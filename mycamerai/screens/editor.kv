
<Editor>:
    name: "Editor"
    AsyncImage:
        id: image
        allow_stretch: True
        keep_ratio: True
        source: self.parent.source
        MDIconButton:
            icon: "account-off-outline"
            id: anonymous_icon
            pos: root.width * 0.1, root.height*0.8
            user_font_size: (dp(96), dp (96))
            theme_text_color: "Primary"
            on_press: root.removing_people_enable()

        MDIconButton:
            icon: "camera-outline"
            id: go_back_to_capture
            pos: root.width * 0.9, root.height*0.8
            user_font_size: (dp(96), dp (96))
            theme_text_color: "Primary"
            on_press: app.manager.current = 'Capture'
